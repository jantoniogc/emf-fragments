<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="benchmark"
    nsURI="http://de.hub.emffrag.benchmark" nsPrefix="bm">
  <eClassifiers xsi:type="ecore:EClass" name="Benchmark" abstract="true">
    <eOperations name="printResult">
      <eParameters name="out" eType="#//PrintStream"/>
      <eParameters name="candidate" eType="#//EMFFragCandidates"/>
      <eParameters name="run" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eOperations>
    <eOperations name="printResultHeader">
      <eParameters name="out" eType="#//PrintStream"/>
    </eOperations>
    <eOperations name="run">
      <eParameters name="candidate" eType="#//EMFFragCandidates"/>
    </eOperations>
    <eOperations name="startTimeMeasure"/>
    <eOperations name="getTimeMeasure" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eOperations name="getMemoryMeasure" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="rMemoryUsage" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="rTime" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="rNumberOfObjects" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="weakUnloads" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="weakLoads" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="loads" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="unloads" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxActiveFagments" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxWeakFragments" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="remarks" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="pModelName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LoadBenchmark" eSuperTypes="#//AbstractGrabatsBenchmark"/>
  <eClassifiers xsi:type="ecore:EClass" name="SaveBenchmark" eSuperTypes="#//AbstractGrabatsBenchmark"/>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractGrabatsBenchmark" eSuperTypes="#//Benchmark">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="pSourceXmiURI" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="frag2" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="false"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="PrintStream" instanceClassName="java.io.PrintStream"
      serializable="false"/>
  <eClassifiers xsi:type="ecore:EClass" name="Measurement">
    <eOperations name="run">
      <eParameters name="measure" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="candidates" upperBound="-1"
        eType="#//EMFFragCandidates"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="benchmarks" upperBound="-1"
        eType="#//Benchmark" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="runs" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="constraints" upperBound="-1"
        eType="#//IConstraint" transient="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="numberOfBenchmarks" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
        derived="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="EMFFragCandidates">
    <eLiterals name="xmi"/>
    <eLiterals name="cdo" value="1"/>
    <eLiterals name="morsa" value="2"/>
    <eLiterals name="emffrag" value="3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="IConstraint" instanceClassName="de.hub.emffrag.bm.benchmark.util.IConstraint"/>
  <eClassifiers xsi:type="ecore:EClass" name="QueryBenchmark" eSuperTypes="#//AbstractGrabatsBenchmark">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="withIndex" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ImportTestModel" eSuperTypes="#//Benchmark">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="fragmentSize" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="modelSize" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LoadFromTestModel" eSuperTypes="#//Benchmark">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="loadSize" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="importBenchmark" eType="#//ImportTestModel"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ImportDeepTestModel" eSuperTypes="#//ImportTestModel">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="base" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
</ecore:EPackage>
